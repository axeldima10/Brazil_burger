<!DOCTYPE html>
<html lang="fr"><!-- Basic -->
<head>
	<meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">   
   
    <!-- Mobile Metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
 
     <!-- Site Metas -->
    <title>{% block title %}Acceuil{% endblock %}</title> 
    <meta name="keywords" content="">
    <meta name="description" content="">
    <meta name="author" content="">

    {% block stylesheets %}
            <!-- Site Icons -->
        <link rel="shortcut icon" href="{{asset('asset1/images/favicon.ico')}}" type="image/x-icon">
        <link rel="apple-touch-icon" href="{{asset('asset1/images/apple-touch-icon.png')}}">

        <!-- Bootstrap CSS -->
        <link rel="stylesheet" href="{{asset('asset1/css/bootstrap.min.css')}}">    
        <!-- Site CSS -->
        <link rel="stylesheet" href="{{asset('asset1/css/style.css')}}">    
        <!-- Responsive CSS -->
        <link rel="stylesheet" href="{{asset('asset1/css/responsive.css')}}">
        <!-- Custom CSS -->
        <link rel="stylesheet" href="{{asset('asset1/css/custom.css')}}">

        <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
        <![endif]-->
    {% endblock %}

</head>

<body>
	<!-- Start header -->
	<header class="top-navbar">
		<nav class="navbar navbar-expand-lg navbar-light bg-light">
			<div class="container">
				<a class="navbar-brand" href="index.html">
					<img src="images/logo.png" alt="" />
				</a>
				<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbars-rs-food" aria-controls="navbars-rs-food" aria-expanded="false" aria-label="Toggle navigation">
				  <span class="navbar-toggler-icon"></span>
				</button>
				<div class="collapse navbar-collapse" id="navbars-rs-food">
					<ul class="navbar-nav ml-auto">
						<li class="nav-item {{ hactive is defined ? hactive : ''}}"><a class="nav-link" href="{{path("home")}}"><i class="fa fa-home"></i> Acceuil</a></li>
						<li class="nav-item {{ mactive is defined ? mactive : ''}}"><a class="nav-link" href="{{path('menu')}}"><i class="fa fa-bars"></i> Nos repas</a></li>
						<li class="nav-item dropdown">
							<a class="nav-link dropdown-toggle" href="#" id="dropdown-a" data-toggle="dropdown"><i class="fa fa-cog"></i> Paramètres</a>
							<div class="dropdown-menu" aria-labelledby="dropdown-a">

							{% if app.user %}
								<a class="dropdown-item" href="{{path("show_profil")}}"><i class="fa fa-user"></i> profil</a>
								<a class="dropdown-item" href="{{path("app_logout")}}"><i class="fa fa-sign-out"></i> se déconnecter</a>
							{% else %}
								<a class="dropdown-item" href="{{path("app_login")}}"><i class="fa fa-sign-in"></i> se connecter</a>
							{% endif %}
								
							</div>
						</li>
						<li class="nav-item {{ cactive is defined ? cactive : ''}}"><a class="nav-link" href="{{path('commande')}}"><i class="fa fa-cart-arrow-down"></i>
						<span class="badge badge-danger badge-counter" id="panier">
							{% if app.session.get('nombre') is null %}
								0
							{% else %}
								{{app.session.get('nombre')}}
							{% endif %}
						</span></a></li>
						<li class="nav-item {{ Cactive is defined ? Cactive : ''}}"><a class="nav-link" href="{{path('contact')}}"><i class="fa fa-address-book"></i> Contact</a></li>
						{% if app.user %}
							{% if "ROLE_GESTIONNAIRE" in app.user.roles %}
								<li class="nav-item"><a class="nav-link" href="{{path("admin")}}"> administration</a></li>
							{% else %}
								<li class="nav-item {{ vactive is defined ? vactive : ''}}"><a class="nav-link" href="{{path("client_commande")}}"> vos commandes</a></li>
							{% endif %}
						{% endif %}
					</ul>
				</div>
			</div>
		</nav>
	</header>
	<!-- End header -->

	{% block body %}{% endblock %}

	<!-- Start QT -->
	<div class="qt-box qt-background">
		<div class="container">
			<div class="row">
				<div class="col-md-8 ml-auto mr-auto text-center">
					<p class="lead ">
						" Si tu n'es pas celui qui cuisine, écarte toi et compliment le chef. "
					</p>
					<span class="lead">Michael Strahan</span>
				</div>
			</div>
		</div>
	</div>
	<!-- End QT -->


	<!-- Start Contact info -->
	<div class="contact-imfo-box">
		<div class="container">
			<div class="row">
				<div class="col-md-4 arrow-right">
					<i class="fa fa-volume-control-phone"></i>
					<div class="overflow-hidden">
						<h4>Téléphone</h4>
						<a class="lead text-light" href="tel:00 221 77 737 01 01">
							+221  77 737 01 01
						</a>
					</div>
				</div>
				<div class="col-md-4 arrow-right">
					<i class="fa fa-envelope"></i>
					<div class="overflow-hidden">
						<h4>Email</h4>
						<a class="lead text-light" href="mailto:brasilburger@gmail.com">
							brasilburger@gmail.com
						</a>
					</div>
				</div>
				<div class="col-md-4">
					<i class="fa fa-map-marker"></i>
					<div class="overflow-hidden">
						<h4>Lieu</h4>
						<p class="lead">
							Dakar, Point E 
						</p>
					</div>
				</div>
			</div>
		</div>
	</div>
	<!-- End Contact info -->

    <!-- Start Footer -->
	<footer class="footer-area bg-f">
		<div class="container">
			<div class="row">
				<div class="col-lg-3 col-md-6">
					<h3>A propos de nous</h3>
					<p>Nous sommes dans le domaine de la vente des burger depuis un certain temps et nous vous offrons un service de qualité. Nous vous livrons à domicile</p>
				</div>
				<div class="col-lg-3 col-md-6">
					<h3>Suivez nous</h3>
					<div class="subscribe_form">
						<form class="subscribe_form">
							<input name="EMAIL" id="subs-email" class="form_input" placeholder="Email Address..." type="email">
							<button type="submit" class="submit">SOUSCRIPTION</button>
							<div class="clearfix"></div>
						</form>
					</div>
					<ul class="list-inline f-social">
						<li class="list-inline-item"><a href="https://www.facebook.com/brasilburger_senegal-102549748217138/"><i class="fa fa-facebook" aria-hidden="true"></i></a></li>
						<li class="list-inline-item"><a href="https://www.google.com/search?client=firefox-b-d&q=brazzil+burger+dakar"><i class="fa fa-google-plus" aria-hidden="true"></i></a></li>
						<li class="list-inline-item"><a href="https://www.instagram.com/brasilburger_senegal/?hl=en"><i class="fa fa-instagram" aria-hidden="true"></i></a></li>
					</ul>
				</div>
				<div class="col-lg-3 col-md-6">
					<h3>Contactez nous</h3>
					<p class="lead">Sénégal, Dakar, Point E, 11000</p>
					<p class="lead"><a href="tel:+221 33 585 89 89">+221 33 585 89 89</a></p>
					<p><a href="mailto:infobrasil@yahoo.fr"> infobrasil@yahoo.fr</a></p>
				</div>
				<div class="col-lg-3 col-md-6">
					<h3>Heures ouvertures</h3>
					<p><span class="text-color">Lundi: </span>Fermé</p>
					<p><span class="text-color">Mardi-Dimanche :</span> 12:00 - 23:30</p>
				</div>
			</div>
		</div>
		
		<div class="copyright">
			<div class="container">
				<div class="row">
					<div class="col-lg-12">
						<p class="company-name">Droits réservés. &copy; 2021 <a href="{{path("home")}}">Brasil Burger</a> réalisé par : 
					<a href="mailto:yameogoivan10@gmail.com">TnT</a></p>
					</div>
				</div>
			</div>
		</div>
		
	</footer>
	<!-- End Footer -->
	
	<a href="#" id="back-to-top" title="Back to top" style="display: none;"><i class="fa fa-paper-plane-o" aria-hidden="true"></i></a>

    {% block javascripts %}
        <!-- ALL JS FILES -->
        <script src="{{asset('asset1/js/jquery-3.2.1.min.js')}}"></script>
        <script src="{{asset('asset1/js/popper.min.js')}}"></script>
        <script src="{{asset('asset1/js/bootstrap.min.js')}}"></script>
        <!-- ALL PLUGINS -->
        <script src="{{asset('asset1/js/jquery.superslides.min.js')}}"></script>
        <script src="{{asset('asset1/js/images-loded.min.js')}}"></script>
        <script src="{{asset('asset1/js/isotope.min.js')}}"></script>
        <script src="{{asset('asset1/js/baguetteBox.min.js')}}"></script>
        <script src="{{asset('asset1/js/form-validator.min.js')}}"></script>
        <script src="{{asset('asset1/js/contact-form-script.js')}}"></script>
        <script src="{{asset('asset1/js/custom.js')}}"></script>
    {% endblock %}
</body>
</html>
